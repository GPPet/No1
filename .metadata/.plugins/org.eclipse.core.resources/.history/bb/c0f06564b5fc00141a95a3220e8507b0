package may13th.Person;

public class Person {
	String name;
	String eMail;
	int age;
	
	public Person(String name, int age, String eMail){
		this.name = "";
		this.age = 0;
		this.eMail = "";
	}
	
	public void printInfo(){
		System.out.println("Name:\t" + name + "\nAge:\t" + age + "\nE-mail:\t" + eMail);
	}

}
