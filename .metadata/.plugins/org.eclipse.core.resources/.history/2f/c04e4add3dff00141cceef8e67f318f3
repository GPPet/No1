import java.util.Random;


public class hw8_P3 {

	public static void main(String[] args) {
		Random rand = new Random();
		int a, b, c;
		int count = 0;
		long prod = 1;
		
		while (count < 20){
			a = rand.nextInt();
			b = rand.nextInt();
			c = rand.nextInt();
			
			if ((a != 0) && (b != 0) && (c != 0)){
				prod *= multiplyThreeInt(a, b, c);
				count++;
			} 
			
		}
			 System.out.println(prod);
		
		

	}
	
	public static long multiplyThreeInt(int a, int b, int c){
		long prod = a*b*c;
		return prod;
	}

}
