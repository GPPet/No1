package stock;

public class Drinks extends Stock{
	protected boolean alcohol;
	protected int bottles;
	
	public Drinks(){
		super();
		this.alcohol = false;
		this.bottles = 0;
	}

	public Drinks(int price, boolean available, boolean alcohol, int bottles){
		super(price, available);
		
		setAlcohol(alcohol);
		setBottles(bottles);
	}
	
	public boolean getAlcohol(){
		return alcohol;
	}
	public void setAlcohol(boolean alcohol){
		this.alcohol = alcohol;
	}
	
	public int getBottles(){
		return bottles;
	}
	public void setBottles(int bottles){
		this.bottles = bottles;
	}
}
