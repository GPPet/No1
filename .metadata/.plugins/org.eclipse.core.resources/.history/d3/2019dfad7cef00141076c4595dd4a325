import java.util.Scanner;

public class Test {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in, "UTF-8");
		
		System.out.println("Please, enter a word");
		String word = input.nextLine();
		int wordSize = word.length();
		System.out.println("Word size: " + wordSize);
		
		boolean wordSizeIsLessThan10 = wordSize < 10;
        System.out.print("Check whether word size < 10: ");
        System.out.println(wordSizeIsLessThan10);
		
		
		System.out.println("Please, enter two integers");
		int a = input.nextInt();
		int b = input.nextInt();
		
		boolean aIsMoreThanB = a > b;
        System.out.print("Check whether a > b: ");
        System.out.println(aIsMoreThanB);
		
        boolean bInRange = (b >= -150) && (b < 15);
        System.out.print("Check whether b is in the range [-150;15): ");
        System.out.println(bInRange);
        
		float remain = a % b; 
		System.out.println(remain);
		
		boolean aAndBLogic = (a > 10) || (b < 88);
        System.out.print("Check whether a>10 or b<88: ");
        System.out.println(aAndBLogic);
		
		
	}
}
