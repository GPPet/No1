	private static int[] permuteIntArray(int[] arr, int size) {
	    int count = 0;
		if (size == 1) {
	        count++;
	        return;
	    }
	    for (int i = 0; i < size; i++) {
	        swap(a, i, n-1);
	        permute(a, n-1);
	        swap(a, i, n-1);
	    }
	    return arr;
	}
	
}
