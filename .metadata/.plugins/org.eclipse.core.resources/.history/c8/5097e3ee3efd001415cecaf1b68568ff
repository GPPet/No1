package stock;

public class Meat extends Stock{
	private int kilo;
	protected boolean frozen;
	
	public Meat(){
		super();
		this.kilo = 0;
		this.frozen = false;
	}
	
	public Meat(int price, boolean available, int kilo, boolean frozen){
		super(price, available);
		
		setKilo(kilo);
		setFrozen(frozen);
	}
	
	public int getKilo(){
		return kilo;
	}
	public void setKilo(int kilo){
		this.kilo = kilo;
	}
	
	public boolean getFrozen(){
		return frozen;
	}
	public void setFrozen(boolean frozen){
		this.frozen = frozen;
	}

}
